{
  "manifest_version": 2,

  "name": "alpha-QVsecure",
  "description": "The QuantaVoice extension enables secure communication, sharing, and blogging.",
  "version": "0.1",

  "browser_action": {
    "default_icon": "qvicon.png",
    "default_popup": "popup.html",
    "default_title": "QuantaVoice"
  },
  "background": {
    "scripts": ["qvsecure.js","qvcontextmenu.js"]
  },
"icons": {
	"16": "qvicon.png",
	"48": "qvicon48.png",
	"128": "qvicon128.png"
},
  "permissions": [
    "http://*.quantavoice.com/", 
    "https://*.quantavoice.com/",
    "https://www.woodsidelabs.com/",
    "http://nominatim.openstreetmap.org/",
    "https://ajax.googleapis.com/",
    "activeTab",
    "contextMenus",
    "geolocation",
    "storage"
  ]
}
